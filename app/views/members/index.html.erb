<p style="color: green"><%= notice %></p>

<h1>Members</h1>

<div id="members">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">First</th>
        <th scope="col"></th>

        <th scope="col">Last</th>
        <th scope="col"></th>

        <th scope="col">Age</th>
        <th scope="col"></th>
      </tr>
    </thead>
    <tbody>
      <tr>
      <% @members.each do |member| %>                    
        <td><%= member.first_name %><td>
        <td><%= member.last_name %><td>
        <td><%= member.age %><td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>

<%= link_to "New member", new_member_path %>
